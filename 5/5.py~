import math


x = 2520
""" This was given as the solution for this problem for 1..10, 
we can use it as a baseline to speed things up a bit"""

def test(n):
    for x in range(1, 21):
        n /= x
   
    if n % 1 == 0:
        return True
    else:
        return False

num = 0
while not num:
    if test(x):
        num = x
        
    x +=1

print num
